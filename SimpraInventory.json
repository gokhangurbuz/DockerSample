{
  "family": "SimpraInventory",
  "containerDefinitions": [
    {
      "image": "protelclearsightapi/clearsight:v_%BUILD_NUMBER%",
      "name": "SimpraInventory",
      "cpu": 10,
      "memory": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 80
        }
      ]
    }
  ]
}
